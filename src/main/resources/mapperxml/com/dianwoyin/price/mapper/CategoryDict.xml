<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com/dianwoyin/price/mapper.CategoryDictMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com/dianwoyin/price/model.CategoryDict">
        <id column="id" property="id" />
        <result column="category_name" property="categoryName" />
        <result column="parent_id" property="parentId" />
        <result column="img_url" property="imgUrl" />
        <result column="leaf" property="leaf" />
        <result column="seq" property="seq" />
        <result column="deleted" property="deleted" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, category_name, parent_id, img_url, leaf, seq, deleted, create_time, update_time
    </sql>

</mapper>
